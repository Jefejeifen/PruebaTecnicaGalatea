          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">import groovy.json.JsonOutput
import java.util.Random

Random rand = new Random()
int size = 6  // tamaño de la matriz
String alphabet = &quot;ABCDEFGHIJKLMNOPQRSTUVWXYZ&quot;

// función que crea una fila aleatoria
def randomRow = { -&gt;
    (1..size).collect { alphabet[rand.nextInt(alphabet.size())] }.join()
}

// inicializamos manuscrito
def manuscript = (1..size).collect { randomRow() }

// 50% de probabilidad de tener pista
boolean hasClue = rand.nextBoolean()

if (hasClue) {
    int rowIndex = rand.nextInt(size)       // fila aleatoria
    int startIndex = rand.nextInt(size - 3) // inicio para 4 letras seguidas
    char letter = alphabet[rand.nextInt(alphabet.size())]

    // convertir a lista para editar
    def chars = manuscript[rowIndex].toCharArray()
    (0..&lt;4).each { i -&gt; chars[startIndex + i] = letter }
    manuscript[rowIndex] = new String(chars)
}

// construir JSON
def json = JsonOutput.toJson([manuscript: manuscript])

// guardar variable en JMeter
vars.put(&quot;RANDOM_BODY&quot;, json)

// opcional: log para depuración
log.info(&quot;Generated manuscript (${hasClue ? &apos;HAS CLUE&apos; : &apos;NO CLUE&apos;}): &quot; + json)
</stringProp>
